function lateRide(n) {
  let res = 0;
  
  if ( n < 0 ) return res;
  if ( n > 1440 ) return res;

  let minHour = Math.floor (n / 60);
  let minMinute = Math.floor(((n /60) - minHour) * 60);
  
  let strRes = String(minHour) + String(minMinute);
    
  for (var i = 0; i < strRes.length; i++) res = res + Number(strRes[i]);
   
  return res;
}
